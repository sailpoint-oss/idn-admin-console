{
	"name": "idn-admin-console",
	"version": "0.0.1",
	"private": true,
	"description": "",
	"main": "src/electron.cjs",
	"type": "module",
	"author": {
		"name": "Luke Hagar",
		"email": "luke.hagar@sailpoint.com"
	},
	"build": {
		"directories": {
			"output": "dist"
		},
		"files": [
			"src/electron.cjs",
			"src/preload.cjs",
			{
				"from": "build",
				"to": ""
			}
		],
		"linux": {
			"category": "Utility",
			"target": "deb"
		},
		"win": {
			"target": "portable"
		}
	},
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"wait-on http://localhost:3000/ && npm run dev:electron\"",
		"dev:svelte": "vite dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:svelte": "vite build",
		"build:electron": "electron-builder -mwl"
	},
	"engines": {
		"npm": ">=7",
		"yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"electron-context-menu": "^3.6.1",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.2.0",
		"electron-window-state": "^5.0.3",
		"wait-on": "^7.1.0"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.5.0",
		"@skeletonlabs/tw-plugin": "^0.2.4",
		"@sveltejs/adapter-static": "2.0.1",
		"@sveltejs/kit": "1.27.5",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"alasql": "^4.2.2",
		"autoprefixer": "^10.4.16",
		"axios": "^1.6.1",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"electron": "^23.3.13",
		"electron-builder": "^24.6.4",
		"electron-connect": "^0.6.3",
		"electron-packager": "^17.1.2",
		"electron-updater": "^5.3.0",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsonwebtoken": "^9.0.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sailpoint-api-client": "^1.3.0",
		"sass": "^1.69.5",
		"svelte": "^3.59.2",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^4.5.0"
	},
	"overrides": {
		"electron": {
			"got": "^12.5.1"
		}
	}
}
